        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="85.0" layoutY="31.0" prefHeight="83.0" prefWidth="469.0" style="&#10;" text="Şifreleme yapmak istediğiniz nesne türünü seçin :))" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Button fx:id="textEncodeButton" layoutX="178.0" layoutY="158.0" mnemonicParsing="false" onAction="#textEncodeClick" prefHeight="58.0" prefWidth="125.0" text="Text / Metin" />
                <Button fx:id="fileEncodeButton" layoutX="365.0" layoutY="158.0" mnemonicParsing="false" onAction="#fileEncodeClick" prefHeight="58.0" prefWidth="103.0" text="File / Dosya" />
